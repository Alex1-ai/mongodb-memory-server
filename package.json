{
  "name": "mongodb-memory-server",
  "version": "0.0.0-development",
  "description": "In-memory MongoDB Server. Designed with testing in mind, the server will allow you to connect your favourite ODM or client library to the MongoDB Server and run integration tests isolated from each other.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/nodkz/mongodb-memory-server",
  "author": "Nodkz (pavel.chertorogov@gmail.com)",
  "files": [
    "lib",
    "postinstall.js"
  ],
  "keywords": [
    "mongodb",
    "mongoose",
    "mock",
    "stub",
    "mockgoose",
    "mongodb-prebuilt",
    "mongomem"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodkz/mongodb-memory-server/issues"
  },
  "homepage": "https://github.com/nodkz/mongodb-memory-server",
  "devDependencies": {
    "@types/decompress": "^4.2.3",
    "@types/dedent": "^0.7.0",
    "@types/find-cache-dir": "^2.0.0",
    "@types/get-port": "^4.0.1",
    "@types/getos": "^3.0.0",
    "@types/jest": "^24.0.9",
    "@types/lockfile": "^1.0.1",
    "@types/md5-file": "^4.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mongodb": "^3.1.19",
    "@types/node": "^10.12.18",
    "@types/tmp": "0.0.34",
    "@types/uuid": "^3.4.4",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "mongodb": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "decompress": "^4.2.0",
    "dedent": "^0.7.0",
    "find-cache-dir": "^2.0.0",
    "get-port": "^4.1.0",
    "getos": "^3.1.1",
    "https-proxy-agent": "^2.2.1",
    "lockfile": "^1.0.4",
    "md5-file": "^4.0.0",
    "mkdirp": "^0.5.1",
    "tmp": "^0.0.33",
    "uuid": "^3.2.1"
  },
  "peerDependencies": {
    "mongodb": ">= 3.0"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:ts": "rimraf build && tsc -p .",
    "watch": "cross-env MONGOMS_DOWNLOAD_DIR=./tmp jest --watch",
    "coverage": "cross-env MONGOMS_DOWNLOAD_DIR=./tmp jest --coverage",
    "lint": "eslint --ext .js ./src && npm run tslint",
    "test": "jest src/__tests__/replset-test.ts",
    "tscheck": "tsc",
    "tslint": "tslint -p . \"src/**/*\"",
    "semantic-release": "semantic-release",
    "postinstall": "node ./postinstall.js"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
